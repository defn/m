{
  "compilerOptions": {
    "paths": {},
    "rootDirs": ["..", "./types"],
    "isolatedModules": true,
    "ignoreDeprecations": "5.0",
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext"
  },
  "include": [
    "ambient.d.ts",
    "./types/**/$types.d.ts",
    "../vite.config.ts",
    "../src/**/*.js",
    "../src/**/*.ts",
    "../src/**/*.svelte",
    "../tests/**/*.js",
    "../tests/**/*.ts",
    "../tests/**/*.svelte"
  ],
  "exclude": [
    "../node_modules/**",
    "./[!ambient.d.ts]**",
    "../src/service-worker.js",
    "../src/service-worker.ts",
    "../src/service-worker.d.ts"
  ]
}
